<Application x:Class="SnailPass_Desctop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SnailPass_Desctop"
             StartupUri="View\LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="IconsDictionary"
                                    Source="view/res/icons/Icons.xaml"/>
                <ResourceDictionary x:Name="ButtonsDictionary"
                                    Source="view/res/Buttons.xaml"/>
                <ResourceDictionary x:Name="FontsDictionary"
                                    Source="view/res/fonts/Fonts.xaml"/>
                <ResourceDictionary x:Name="ImagesDictionary"
                                    Source="view/res/images/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
