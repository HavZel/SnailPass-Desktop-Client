<Application
    x:Class="SnailPass.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="FontsDictionary" Source="view/res/fonts/Fonts.xaml" />
                <ResourceDictionary x:Name="ColorsDictionary" Source="view/res/Colors.xaml" />
                <ResourceDictionary x:Name="IconsDictionary" Source="view/res/icons/Icons.xaml" />
                <ResourceDictionary x:Name="ButtonsDictionary" Source="view/res/Buttons.xaml" />
                <ResourceDictionary x:Name="ImagesDictionary" Source="view/res/images/Images.xaml" />
                <ResourceDictionary x:Name="BaseWindowElements" Source="view/res/WindowElements.xaml" />
                <ResourceDictionary x:Name="SliderDictionary" Source="view/res/Slider.xaml" />
                <ResourceDictionary x:Name="ScrollBar" Source="view/res/ScrollBar.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
